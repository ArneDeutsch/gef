<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.gef4.mvc.examples"
      label="GEF4 MVC Examples"
      version="0.2.0.qualifier"
      provider-name="Eclipse GEF"
      plugin="org.eclipse.gef4.mvc.examples"
      license-feature="org.eclipse.license"
      license-feature-version="1.0.1">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.fx.runtime.min.feature" version="2.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.gef4.common" version="0.1.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.geometry" version="0.1.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.geometry.convert.fx" version="0.1.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.fx" version="0.2.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.fx.ui" version="0.1.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.mvc" version="0.2.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.mvc.fx" version="0.2.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.mvc.fx.ui" version="0.2.0" match="equivalent"/>
      <import feature="org.eclipse.gef4.mvc.ui" version="0.2.0" match="equivalent"/>
      <import plugin="com.google.guava" version="12.0.0" match="greaterOrEqual"/>
      <import plugin="com.google.inject" version="3.0.0" match="compatible"/>
      <import plugin="com.google.inject.multibindings" version="3.0.0" match="compatible"/>
      <import plugin="javax.inject" version="1.0.0" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.gef4.mvc.examples.logo"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.gef4.mvc.examples.logo.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.gef4.mvc.examples"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
