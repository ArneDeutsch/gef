<feature
      id="org.eclipse.gef4.zest.examples"
      label="GEF4 Zest Examples"
      version="1.0.0.qualifier"
      provider-name="Eclipse GEF"
      plugin="org.eclipse.gef4.zest.examples"
      license-feature="org.eclipse.license"
      license-feature-version="1.0.1">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.fx.runtime.min.feature" version="2.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.gef4.common" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.fx" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.geometry" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.geometry.convert.fx" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.graph" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.layout" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.mvc" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.mvc.ui" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.mvc.fx" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.mvc.fx.ui" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.zest.fx" version="1.0.0" match="compatible"/>
      <import feature="org.eclipse.gef4.zest.fx.ui" version="1.0.0" match="compatible"/>
      <import plugin="com.google.guava" version="12.0.0" match="greaterOrEqual"/>
      <import plugin="com.google.inject" version="3.0.0" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.gef4.zest.examples.graph"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.gef4.zest.examples.graph.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.gef4.zest.examples"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
